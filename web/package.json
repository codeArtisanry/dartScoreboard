{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix",
    "prepare": "cd .. && husky install web/.husky",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.5",
    "bootstrap": "^4.6.1",
    "bootstrap-vue": "^2.21.2",
    "cookie-parser": "^1.4.6",
    "cookie-universal-nuxt": "^2.1.5",
    "cookieparser": "^0.1.0",
    "core-js": "^3.19.3",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "nuxt": "^2.15.8",
    "nuxt-parse": "^0.1.3",
    "vue": "^2.6.14",
    "vue-cookies": "^1.7.4",
    "vue-multiselect": "^2.1.6",
    "vue-server-renderer": "^2.6.14",
